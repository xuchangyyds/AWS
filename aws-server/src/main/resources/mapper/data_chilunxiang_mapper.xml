<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aws.mapper.data.ChilunxiangMapper">

    <select id="queryNewestChilunxiangByTurbineId" resultType="com.aws.po.DataChilunxiang"
            parameterType="java.lang.String">
        SELECT *
        FROM data_chilunxiang
        WHERE turbine_id = #{windTurbineId}
        ORDER BY record_time DESC
        LIMIT 1;
    </select>
</mapper>