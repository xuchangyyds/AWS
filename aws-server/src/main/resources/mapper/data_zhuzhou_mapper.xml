<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aws.mapper.data.ZhuzhouMapper">

    <select id="queryNewestZhuzhouByTurbineId" resultType="com.aws.po.DataZhuzhou" parameterType="java.lang.String">
        SELECT * FROM data_zhuzhou
        WHERE turbine_id = #{turbineId}
        ORDER BY record_time DESC
        LIMIT 1;
    </select>


</mapper>